{
    "name": "wujunze/money-wrapper",
    "description": "MoneyPHP Wrapper",
    "type": "library",
    "keywords": ["money", "php", "wrapper", "package", "laravel"],
    "license": "MIT",
    "authors": [
        {
            "name": "wujunze",
            "email": "itwujunze@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "WuJunze\\MoneyWrapper\\": "src/"
        },
        "files": [
            "src/Support/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "WuJunze\\MoneyWrapper\\Tests\\": "tests/"
        }
    },
    "require": {
        "php": ">=7.0",
        "illuminate/support": "^5.5",
        "moneyphp/money": "^3.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.5",
        "orchestra/testbench": "~3.0",
        "codedungeon/phpunit-result-printer": "^0.4.4"
    },
    "extra": {
        "laravel": {
            "providers": [
                "WuJunze\\MoneyWrapper\\MoneyWrapperServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
